<% include partial/header %>

<h1>The Posts Page!</h1>
<% for(var i = 0; i < posts.length; i++) { %>
<li>
	<%= posts[i].title %> - <strong><%= posts[i].autor %></strong>
</li>
<% } %>

<br>

<% posts.forEach(function(post){ %>
	<li>
	<%= post.title %> - <strong><%= post.autor %></strong>
	</li>
<% }); %>

<% include partial/footer %>